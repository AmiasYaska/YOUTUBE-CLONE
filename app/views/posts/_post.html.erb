<%= link_to post do %>
  <div id="<%= dom_id post %>">

    <%= image_tag post.video.preview(resize_to_limit: [300, 300]).processed.url %>

    <p class="my-3">
      <strong class="block font-medium mb-1"></strong>
      <%= post.title %>
    </p>

    <p class="my-3">
      Posted by <%= post.user.email %>
    </p>

    <br/>

  </div>
<% end %>

      